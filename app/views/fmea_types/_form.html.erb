<%= simple_form_for(@fmea_type) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :fmea_name %>
    <%= f.input :fmea_description %>
    <%= f.input :fmea_notes %>
    <%= f.input :fmea_active %>
    <%= f.input :fmea_created_id %>
    <%= f.input :fmea_updated_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
