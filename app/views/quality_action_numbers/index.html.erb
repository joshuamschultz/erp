<h1>Listing quality_action_numbers</h1>

<table>
  <tr>
    <th>Next action no</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @quality_action_numbers.each do |quality_action_number| %>
  <tr>
    <td><%= quality_action_number.next_action_no %></td>
    <td><%= link_to 'Show', quality_action_number %></td>
    <td><%= link_to 'Edit', edit_quality_action_number_path(quality_action_number) %></td>
    <td><%= link_to 'Destroy', quality_action_number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

