<h1>Listing item_processes</h1>

<table>
  <tr>
    <th>Item</th>
    <th>Process type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @item_processes.each do |item_process| %>
  <tr>
    <td><%= item_process.item %></td>
    <td><%= item_process.process_type %></td>
    <td><%= link_to 'Show', item_process %></td>
    <td><%= link_to 'Edit', edit_item_process_path(item_process) %></td>
    <td><%= link_to 'Destroy', item_process, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

