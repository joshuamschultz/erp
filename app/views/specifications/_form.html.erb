<%= simple_form_for(@specification) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :specification_active %>
    <%= f.input :specification_created_id %>
    <%= f.input :specification_updated_id %>
    <%= f.input :specification_identifier %>
    <%= f.input :specification_description %>
    <%= f.input :specification_notes %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
