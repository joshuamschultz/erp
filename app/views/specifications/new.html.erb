<h4 class="heading">New Specification</h4>

<%= render 'form' %>

<script>
$(function()
{

  $("#specification_attachment_attributes_attachment_revision_date").datepicker().on("keydown", function(e){
  	if (e.which == 13) {
      var n=$(this).closest("form").attr("id");
      var s=tab_field_forms[n].indexOf($(this).attr("id"));
      $($("#"+tab_field_forms[n][s]).selector).datepicker('hide');
  	}
  });
});

</script>
