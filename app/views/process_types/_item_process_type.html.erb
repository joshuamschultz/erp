

<!-- Widget -->
<div class="widget">
    <!-- Widget heading -->
    <div class="widget-head">
        <div class="row-fluid">
            <h4 class="span6 heading">Process Type</h4>
            <span class="span6 heading right">
            <% if can? :edit, ProcessType %>
                <%= link_to 'New  Process Type', new_process_type_path, :class => "btn btn-mini btn-success" %>
            <% end %>
            </span>
        </div> 
    </div>
    <!-- // Widget heading END -->

    <div class="widget-body">
        <table id="table_report_process_types" class="dynamicTable table table-striped table-bordered table-condensed">
            <thead>
              <tr>
                <th>Name</th>
                <th>Effective Date</th>
                <th>Public</th>
                <th>Updated By</th>
                <th></th>
              </tr>
            </thead>

            <tbody>

            </tbody>
        </table>
    </div>
</div>
<%= render :partial => 'layouts/app_layouts/data_table', :locals => 
    {:name => "table_report_process_types", :path => process_types_path(item_id:  (@item.nil? ? nil : @item.id)),
        :extras => {:slno => false}, 
        :columns => '[                        
                        { "mData": "attachment_name" },
                        { "mData": "effective_date" },
                        { "mData": "attachment_active" },
                        { "mData": "uploaded_by" },
                        { "mData": "links" }
                    ]'
    } 
%>