<%= simple_form_for(@element) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :element_name %>
    <%= f.input :element_symbol %>
    <%= f.input :element_notes %>
    <%= f.input :element_created_id %>
    <%= f.input :element_updated_id %>
    <%= f.input :element_active %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
