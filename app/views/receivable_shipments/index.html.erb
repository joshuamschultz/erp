<h1>Listing receivable_shipments</h1>

<table>
  <tr>
    <th>Receivable shipment identifier</th>
    <th>Receivable shipment count</th>
    <th>Receivable shipment cost</th>
    <th>Receivable shipment created</th>
    <th>Receivable shipment updated</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @receivable_shipments.each do |receivable_shipment| %>
  <tr>
    <td><%= receivable_shipment.receivable_shipment_identifier %></td>
    <td><%= receivable_shipment.receivable_shipment_count %></td>
    <td><%= receivable_shipment.receivable_shipment_cost %></td>
    <td><%= receivable_shipment.receivable_shipment_created_id %></td>
    <td><%= receivable_shipment.receivable_shipment_updated_id %></td>
    <td><%= link_to 'Show', [receivable_shipment.receivable, receivable_shipment] %></td>
    <td><%= link_to 'Edit', edit_receivable_receivable_shipment_path(receivable_shipment.receivable, receivable_shipment) %></td>
    <td><%= link_to 'Destroy', [receivable_shipment.receivable, receivable_shipment], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Receivable shipment', new_receivable_receivable_shipment_path(params[:receivable_id]) %>
