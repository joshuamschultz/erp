<form style="margin:20px 0" method="post">
    <p>
        <select id="test" multiple="multiple" style="width:370px">
            <option value="1">Red</option>
            <option value="2">Green</option>
            <option value="3">Blue</option>
            <option value="4">Orange</option>
            <option value="5">Purple</option>
            <option value="6">Yellow</option>
            <option value="7">Brown</option>
            <option value="8">Black</option>
        </select>
    </p>

    <p>
        <select multiple="multiple" style="width:370px">
        <optgroup label="test">
            <option value="red">Red</option>
            <option value="green">Green</option>
            <option value="blue">Blue</option>
        </optgroup>
        <optgroup label="foo">
            <option value="orange">Orange</option>
            <option value="purple">Purple</option>
            <option value="yellow">Yellow</option>
            <option value="brown">Brown</option>
            <option value="black">Black</option>
        </optgroup>
        </select>
    </p>

    <input type="hidden" id="sel_values" name="sree">

    <input type="submit" onclick="ok_for()">
</form>

<script type="text/javascript">
    

    $(document).ready(function(){
         $("#test").html('<option value="7">Brown</option><option value="8">Black</option>');
        $("#test").multiselect().multiselectfilter();
    });

    function ok_for(){
        console.log($('#test').multiselectfilter('widget').prevObject.val());
        $("#sel_values").val($('#test').multiselectfilter('widget').prevObject.val());
        alert($("#sel_values").val());
    }

</script>
