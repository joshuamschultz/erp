<div class="row-fluid span8">
    <div class="label label-large label-info arrowed-in arrowed-right">
        <h4>Territory Info</h4>
    </div>
    <table class="table table-striped table-bordered">
        <tbody>
            <tr>
              <td class="span2">Identifier :</td>
              <td class="span5"><%= @territory.territory_identifier %></td>
            </tr>

            <tr>
              <td class="span3">Name :</td>
              <td class="span5"><%= @territory.territory_description %></td>
            </tr>

            <!-- <tr>
              <td class="span3">Zip :</td>
              <td class="span5"><%#= @territory.territory_zip %></td>
            </tr> -->

            <tr>
              <td class="span3">Active :</td>
              <td class="span5"><%= @territory.territory_active %></td>
            </tr>
        </tbody>
    </table>

  <div class="form-actions">
    <%= link_to 'Edit', edit_territory_path(@territory), :class => "btn btn-small" %>
    <%= link_to 'Back to Territories', territories_path, :class => "btn btn-small" %>
  </div>
</div>