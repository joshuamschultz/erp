<h1>Listing territories</h1>

<table>
  <tr>
    <th>Territory active</th>
    <th>Territory created</th>
    <th>Territory updated</th>
    <th>Territory identifier</th>
    <th>Territory description</th>
    <th>Territory zip</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @territories.each do |territory| %>
  <tr>
    <td><%= territory.territory_active %></td>
    <td><%= territory.territory_created_id %></td>
    <td><%= territory.territory_updated_id %></td>
    <td><%= territory.territory_identifier %></td>
    <td><%= territory.territory_description %></td>
    <td><%= territory.territory_zip %></td>
    <td><%= link_to 'Show', territory %></td>
    <td><%= link_to 'Edit', edit_territory_path(territory) %></td>
    <td><%= link_to 'Destroy', territory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Territory', new_territory_path %>
