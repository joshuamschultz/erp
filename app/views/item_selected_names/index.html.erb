<h1>Listing item_selected_names</h1>

<table>
  <tr>
    <th>Item</th>
    <th>Item alt name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @item_selected_names.each do |item_selected_name| %>
  <tr>
    <td><%= item_selected_name.item %></td>
    <td><%= item_selected_name.item_alt_name %></td>
    <td><%= link_to 'Show', item_selected_name %></td>
    <td><%= link_to 'Edit', edit_item_selected_name_path(item_selected_name) %></td>
    <td><%= link_to 'Destroy', item_selected_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

