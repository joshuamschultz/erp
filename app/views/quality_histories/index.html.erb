<h1>Listing quality_histories</h1>

<table>
  <tr>
    <th>Quality lot</th>
    <th>Quality status</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @quality_histories.each do |quality_history| %>
  <tr>
    <td><%= quality_history.quality_lot_id %></td>
    <td><%= quality_history.quality_status %></td>
    <td><%= quality_history.user_id %></td>
    <td><%= link_to 'Show', quality_history %></td>
    <td><%= link_to 'Edit', edit_quality_history_path(quality_history) %></td>
    <td><%= link_to 'Destroy', quality_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Quality history', new_quality_history_path %>
