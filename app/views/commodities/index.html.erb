<h1>Listing commodities</h1>

<table>
  <tr>
    <th>Commodity active</th>
    <th>Commodity created</th>
    <th>Commodity updated</th>
    <th>Commodity identifier</th>
    <th>Commodity description</th>
    <th>Commodity notes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @commodities.each do |commodity| %>
  <tr>
    <td><%= commodity.commodity_active %></td>
    <td><%= commodity.commodity_created_id %></td>
    <td><%= commodity.commodity_updated_id %></td>
    <td><%= commodity.commodity_identifier %></td>
    <td><%= commodity.commodity_description %></td>
    <td><%= commodity.commodity_notes %></td>
    <td><%= link_to 'Show', commodity %></td>
    <td><%= link_to 'Edit', edit_commodity_path(commodity) %></td>
    <td><%= link_to 'Destroy', commodity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

