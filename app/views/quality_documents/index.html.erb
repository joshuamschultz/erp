                <th></th>
              </tr>
            </thead>

            <tbody>

            </tbody>
        </table>
    </div>
</div>

<%= render :partial => 'layouts/app_layouts/data_table', :locals => 
    {
        :name => "table_report", :path => quality_documents_path,
        :columns => '[
                        { "mData": "id" },
                        { "mData": "name" },
                        { "mData": "links" }

                    ]'
    } 
%>
