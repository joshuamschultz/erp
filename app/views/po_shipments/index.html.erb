<h1>Listing po_shipments</h1>

<table>
  <tr>
    <th>Po line</th>
    <th>Shipped item identifier</th>
    <th>Shipped item count</th>
    <th>Shipped item description</th>
    <th>Po shipment active</th>
    <th>Po shipment created</th>
    <th>Po shipment updated</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @po_shipments.each do |po_shipment| %>
  <tr>
    <td><%= po_shipment.po_line %></td>
    <td><%= po_shipment.shipped_item_identifier %></td>
    <td><%= po_shipment.shipped_item_count %></td>
    <td><%= po_shipment.shipped_item_description %></td>
    <td><%= po_shipment.po_shipment_active %></td>
    <td><%= po_shipment.po_shipment_created_id %></td>
    <td><%= po_shipment.po_shipment_updated_id %></td>
    <td><%= link_to 'Show', po_shipment %></td>
    <td><%= link_to 'Edit', edit_po_shipment_path(po_shipment) %></td>
    <td><%= link_to 'Destroy', po_shipment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Po shipment', new_po_shipment_path %>
