<!-- Widget -->
<div class="widget">
    <!-- Widget heading -->
    <div class="widget-head">
        <div class="row-fluid">
            <h4 class="span6 heading">SO Shipments</h4>
            <span class="span6 heading right">
                <%= link_to 'New SO Shipment', new_so_shipment_path, :class => "btn btn-mini btn-success" %>
            </span>
        </div> 
    </div>
    <!-- // Widget heading END -->

    <div class="widget-body">
        <table id="so_shipmenst_table_report" class="dynamicTable table table-striped table-bordered table-condensed">
            <thead>
              <tr>
                <th>ID</th>
                <th>Received</th>
                <th>Shelf</th>
                <th>Unit</th>
              </tr>
            </thead>

            <tbody>
                
            </tbody>
        </table>
    </div>
</div>

<%= render :partial => 'layouts/app_layouts/data_table', :locals => 
    {
        :name => "so_shipmenst_table_report", :path => so_shipments_path,
        :columns => '[
                        { "mData": "id" },
                        { "mData": "so_shipped_count" },
                        { "mData": "so_shipped_shelf" },
                        { "mData": "so_shipped_unit" }
                    ]'
    } 
%>