<div class="row-fluid span8">
  <%= render 'attachments/attachment_info' %>
  <div class="form-actions">
      <%= link_to 'Attachment', @attachment.attachment.url(:original), :class => "btn btn-small", :target => "_blank" %> 
      <%= link_to 'Edit', edit_control_plan_path(@control_plan), :class => "btn btn-small"%> 
      <%= link_to 'Delete', control_plan_path(@control_plan), method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn btn-small" %>
      <%= link_to 'Back', control_plans_path, :class => "btn btn-small" %>
  </div>
</div>