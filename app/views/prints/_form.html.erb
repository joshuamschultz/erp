<%= simple_form_for(@print) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :print_active %>
    <%= f.input :print_created_id %>
    <%= f.input :print_updated_id %>
    <%= f.input :print_identifier %>
    <%= f.input :print_description %>
    <%= f.input :print_notes %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
